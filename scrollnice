```javascript

      var ns = null;
        var nstimer;
        var waite = true;
        $(function(){
            ns =  $("#scrollbox").niceScroll();
            ns.jqbind("#scrollbox","scroll",function(){ if(ns.newscrolly == ns.page.maxh){
                
                if(waite){
                    waite = !waite;
                    clearTimeout(nstimer);
                    nstimer = setTimeout(function(){
                        console.log("adding……");
                        $("#scrollbox>div").append(  $("#scrollbox").html());
                        waite = !waite;
                    }, 300);     
                }
                }});
        });

